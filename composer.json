{
    "name": "codzo/platinum28degree",
    "type": "library",
    "description": "Gadget to interact with 28Degree Credit Card Edit",
    "keywords": ["28degree", "credit", "card"],
    "homepage": "https://github.com/codzo/Platinum28Degree",
    "license": "MIT License",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Neil Fan",
            "email": "neil.fan@codzo.com",
            "homepage": "http://codzo.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "codzo/config":"*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "codzo/phpcodesniffer-git-hook": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Codzo": "src"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "rm -rf .git/hooks",
            "ln -s ../vendor/codzo/phpcodesniffer-git-hook/hooks .git/hooks"
        ]
    }
}
